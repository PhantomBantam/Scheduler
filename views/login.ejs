<h1>Login</h1>

<form action="/users/login" method="POST">
  <input name="email" type="email" id="email-log" placeholder="Email" required>
  <input name="password" type="password" id="password-log" placeholder="Password" required>

  <button type="submit">Login</button>
</form>

<form action="/users/register" method="GET">
  <button>Don't have an account?</button>
</form>
